#include <stdio.h>
#include <stdlib.h>
//SDL close
int main(void) {
    int arr[3][2];
    int i;
    int* p = &arr;


    for (i = 0; i < 6; i++) {
        scanf("%d", &*(p + i));
    }
        

    for (i = 0; i < 3; i++) {
        for (int j = 0; j < 2; j++) {
            printf("%d ", arr[i][j]);
        }
        printf("\n");
    }

	return 0;
}
